{
    "name": "webflorist/htmlfactory",
    "description": "Convenient and powerful HTML-builder for Laravel 5.5",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
		"laravel/framework": ">=5.5"
	},
    "require-dev": {
        "orchestra/testbench": "3.5.*",
        "gajus/dindent": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\HtmlFactory\\": "src/",
            "HtmlFactoryTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\HtmlFactory\\HtmlFactoryServiceProvider"
            ],
            "aliases": {
                "Html": "Webflorist\\HtmlFactory\\HtmlFactoryFacade"
            }
        }
    }
}
